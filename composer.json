{
    "name": "omnipay/rabobank",
    "type": "library",
    "description": "Rabobank Omnikassa driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "ideal",
        "omnikassa",
        "rabobank"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-rabobank",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-rabobank/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Rabobank\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8",
        "omnipay/common": "^3.1"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.1",
        "squizlabs/php_codesniffer": "^3.6.0"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
